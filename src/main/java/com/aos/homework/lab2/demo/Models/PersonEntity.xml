<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.aos.homework.lab2.demo.Models.PersonEntity">
        <table name="person" schema="lab2"/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="name">
                <column name="name" length="45"/>
            </basic>
            <basic name="email">
                <column name="email" nullable="false" length="45"/>
            </basic>
            <basic name="age">
                <column name="age" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
